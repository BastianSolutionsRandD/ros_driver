<!-- depth registered -->
<launch>
  <node pkg="nodelet" type="nodelet" name="nodelet_manager" args="manager" />

  <node pkg="nodelet" type="nodelet" name="nodelet1"
        args="load depth_image_proc/register nodelet_manager" output="screen">
    <remap from="rgb/camera_info" to="/test/camera_info"/>
<!--    <remap from="rgb/camera_info" to="/rgb_camera/raw/camera_info"/>-->

    <remap from="depth/camera_info" to="/stereo_camera/depth/camera_info"/>
    <remap from="depth/image_rect" to="/stereo_camera/depth/image"/>
  </node>
</launch>
